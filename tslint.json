{
    "extends": [
      "tslint:recommended",
      "tslint-config-standard"
    ],
    "rules": {
      "semicolon": [false],
      "no-default-export": [false],
      "variable-name": [false],
      "forin": [false],
      "indent": [
        true,
        "spaces",
        2
      ],
      "await-promise": [ false ],
      "radix": [ false ],
      "no-console": [ false ],
      "interface-name": [ false ],
      "max-line-length": [ false ],
      "no-namespace": [ false ],
      "max-classes-per-file": [ false ],
      "object-literal-sort-keys": [ false ],
      "space-before-function-paren": [ true ]
    }
  }
  